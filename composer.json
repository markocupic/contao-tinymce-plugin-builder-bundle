{
  "name": "markocupic/contao-tinymce-plugin-builder-bundle",
  "type": "contao-bundle",
  "description":"Plugin builder for tinyMCE rte under Contao",
  "keywords":["contao", "plugin", "builder", "tinyMCE"],
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "symfony/framework-bundle": "^3.3",
    "contao/core-bundle": "^4.4.2"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ContaoTinymcePluginBuilderBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ContaoNewsInfiniteScrollBundle\\ContaoManager\\Plugin"
  }
}
