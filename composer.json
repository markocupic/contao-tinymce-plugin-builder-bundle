{
    "name": "markocupic/contao-tinymce-plugin-builder-bundle",
    "type": "contao-bundle",
    "description": "Plugin builder for tinyMCE rte under Contao",
    "keywords": [
        "contao",
        "plugin",
        "builder",
        "tinyMCE"
    ],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Marko Cupic",
            "homepage": "https://github.com/markocupic/contao-tinymce-plugin-builder-bundle"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\ContaoTinymcePluginBuilderBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Markocupic\\ContaoTinymcePluginBuilderBundle\\ContaoManager\\Plugin"
    }
}
